{
  "version": 2,
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.13" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ],
  "buildCommand": "bash build_files.sh"
}
